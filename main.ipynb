   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "def removeNameAddCategory(df, category):\n",
    "    df = df.reindex(columns = ['category'] + df.columns.tolist())\n",
    "    df['category'] = df['category'].fillna(category)\n",
    "    df = df.drop('name', axis = 1)\n",
    "    return df"
   ]
